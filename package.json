    "test": "flow --quiet && cross-env NODE_ENV=test nyc mocha --reporter min backend/test frontend/test",
    "coverage": "flow-coverage-report -i 'backend/lib/**/*.js' -i 'frontend/lib/**/*.js' -t html -o coverage/flow && npm test"
  },
