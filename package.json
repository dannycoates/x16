{
  "name": "x16",
  "version": "1.0.0",
  "description": "",
  "main": "data/bundle.js",
  "scripts": {
    "bash": "bash",
    "start": "NODE_ENV=production npm run addon",
    "lint": "eslint . --ignore-path .gitignore",
    "pack": "cd src; webpack; cd ..;",
    "addon": "npm run pack && jpm post --post-url http://127.0.0.1:8888",
    "tools": "cd tools/control-panel; webpack",
    "slow-test": "flow --quiet && cross-env NODE_ENV=test nyc mocha --reporter min src/test",
    "test": "mocha --compilers js:babel-register --reporter min src/test",
    "coverage": "flow-coverage-report -i 'src/lib/**/*.js' -t html -o coverage/flow && npm run slow-test"
  },
  "author": "",
  "license": "MPL-2.0",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-register": "^6.16.3",
    "cross-env": "^3.1.3",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "flow-bin": "^0.37.4",
    "flow-coverage-report": "^0.2.0",
    "flow-typed": "^2.0.0",
    "jpm": "^1.2.2",
    "mocha": "^3.1.2",
    "nyc": "^10.0.0",
    "prettier": "^0.0.8",
    "proxyquire": "^1.7.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sinon": "^1.17.6",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1",
    "seedrandom": "^2.4.2"
  }
}
